<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="mainLayout" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.cab302groupnametbdproject.HelloController">
    <top>
        <Button text="Sign Out" onAction="#onSignOutButtonClick" BorderPane.alignment="TOP_LEFT"/>
    </top>
    <center>
        <VBox fx:id="mainContent" alignment="CENTER" spacing="20.0">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <Label/>
            <Button text="Associated Websites" onAction="#onAssociatedWebsitesClick"/>
            <Button text="About the App" onAction="#onAboutButtonClick"/>
            <Button text="Child Accounts" onAction="#onChildAccountsButtonClick"/>
        </VBox>
    </center>
</BorderPane>
